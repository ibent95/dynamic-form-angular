{
  "name": "dynamic-form-app",
  "version": "1.1.0",
  "description": "A project to implement a Dynamic Form mechanism in client side.",
  "scripts": {
    "ng": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng",
    "start": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng serve --hmr",
    "build": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng build --base-href=/dynamic-form-angular/ --deploy-url=/dynamic-form-angular/ --configuration=production",
    "watch": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng test",
    "start-ssl": "node --max_old_space_size=20000 ./node_modules/@angular/cli/bin/ng serve --hmr --ssl true --ssl-key ./localhost.key --ssl-cert ./localhost.crt"
  },
